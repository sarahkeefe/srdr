<b>Choose Outcome to View: </b><br/>
	<%= select "outcome", "outcome", Outcome.where(:study_id => session[:study_id]).all.collect {|o| [o.title,o.id]}, {},
					{:onchange =>remote_function(:url => {:action => "show_outcome_subgroups_and_timepoints"}, 
					:with => "'selected_outcome_id=' + this.value + '&form_type=' + $('model_name').value")} %></b>
	  <br/><br/>
	  <input type="hidden" id="model_name" value=<%=@model_name %> >
		<div id="subgroup_options">
		<b>Select a Sub-Group to Analyze</b><br/>
		<% unless @first_subgroups.empty? %>
		  
		  <% subgroups = "" %>
		  <% i = 0 %>
		  <% selected = "selected" %>
		  <% @first_subgroups.each do |sg| %>
		    <% unless i < 1 %>
		       <% selected = "" %>
		    <% end %>
		    <% subgroups = subgroups + "<option value=#{sg.id} #{selected}>#{sg.title}</option>" %>
		    <% i += 1%>
		  <% end %>
			
		  <select name=<%= @model_name + "[subgroup_id]" %> id=<%= @model_name + "_subgroup_id" %>>
		  	<%= render :inline=>subgroups %>
		  </select>
		<% else %>
		  <em>There are no subgroups defined for this outcome. If you require a subgroup, please edit the <%= link_to "Outcome Setup Page", "/projects/#{session[:project_id]}/studies/#{session[:study_id]}/outcomesetup" %></em>
		<% end %>
		</div>
		<br/>
		<div id="timepoint_options">
		<b>Select a Timepoint to Analyze</b><br/>
		<% unless @first_timepoints.empty? %>
		  <% timepoints = "" %>
		  <% i = 0 %>
		  <% selected = "selected" %>
		  <% @first_timepoints.each do |tp| %>
		    <% unless i < 1 %>
		       <% selected = "" %>
		    <% end %>
		    <% timepoints = timepoints + "<option value=#{tp.id} #{selected}>#{tp.number} #{tp.time_unit}</option>" %>
		    <%i += 1%>
		  <% end %>
			
		  <select name=<%= @model_name + "[timepoint_id]" %> id=<%= @model_name + "[timepoint_id]" %>>
		  	<%= render :inline=>timepoints %>
		  </select>
		<% else %>
		  <em>There are no time points defined for this outcome. If you require a time point, please edit the <%= link_to "Outcome Setup Page", "/projects/#{session[:project_id]}/studies/#{session[:study_id]}/outcomesetup" %></em>
		<% end %>		
		</div>
