<!DOCTYPE html>
<html>
<head>
  <title>Srdr</title>
  
<%= stylesheet_link_tag "blueprint/screen.css", :type => "text/css", :media=>"screen, projection"%>
<%= stylesheet_link_tag "blueprint/print.css", :type=>"text/css", :media=>"print"%>
<!--[if lt IE 8]>
  <%= stylesheet_link_tag "blueprint/ie.css", :type=>"text/css", :media=>"screen, projection"%>
<![endif]-->
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= javascript_include_tag 'prototype', 'form_complete' %>
  <script src="/javascripts/scriptaculous/src/scriptaculous.js" type="text/javascript"></script>
  <%= csrf_meta_tag %>
  </head>
<body>

	<div class="container">
		<div class="span-24 last">
			<%= render "layouts/header" %>
		</div>
		<div class="span-3">
			Left sidebar
		</div>
		<div class="span-21 last">
		<%= render "layouts/breadcrumbs" %>
			<%= yield %>
		</div>
	</div>
	
</body>
</html>
