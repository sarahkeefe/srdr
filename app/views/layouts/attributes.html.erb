<!DOCTYPE html>
<html>
<head>
  <title>Srdr</title>

  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= javascript_include_tag 'prototype', 'jquery', 'nested_form' %>
  <%= javascript_include_tag 'pop_chars_totals' %>
  <%= javascript_include_tag 'custom-form-elements' %>
  <script src="/javascripts/form_complete.js" type="text/javascript"></script>
  <%= javascript_include_tag 'validation_alerts' %>
  <script src="/javascripts/scriptaculous/src/scriptaculous.js" type="text/javascript"></script>

  </head>
<body>

	<div class="login-header">
			<%= render "layouts/login" %>
	</div>

	<div class="container">
		<div class="header">
			<%= render "layouts/header" %>
		</div>
		<div class="content">
					<%= render "layouts/breadcrumbs" %>
			<%= yield %>
		</div>
	</div>

</body>
</html>
