
<table class="list_table">
<th>Number</th>
<th>Question Text</th>
<th>Description</th>
<th>Move Up</th>
<th>Edit</th>
<th>Remove</th>
<% if !@key_questions.nil? && @key_questions.length > 0 %>
<% for keyq in @key_questions %>
<tr class="<%= cycle("even", "odd") %>" id=<%= "kq_row_"+keyq.question_number.to_s %>>
<td><%= keyq.question_number %></td>
<td><%= keyq.question %></td>
<td><%= keyq.description %></td>
<td>
<% proj_id = session[:project_id] %>
<%= link_to "move up" %>
</td>
<td>
	<%= link_to 'Edit', edit_project_key_question_path(session[:project_id],keyq), :remote=>true %>
</td>
<td><%= link_to 'Delete', key_question_path(keyq), :remote=>true, :confirm => 'Are you sure?', :method=>:delete, :class=>"kq_delete_link" %></td>
</tr><% end %>
<% end %>
</table>