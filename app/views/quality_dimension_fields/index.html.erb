<h1>Listing quality_dimension_fields</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Field notes</th>
    <th>Template</th>
    <th>Study</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @quality_dimension_fields.each do |quality_dimension_field| %>
  <tr>
    <td><%= quality_dimension_field.title %></td>
    <td><%= quality_dimension_field.field_notes %></td>
    <td><%= quality_dimension_field.template_id %></td>
    <td><%= quality_dimension_field.study_id %></td>
    <td><%= link_to 'Show', quality_dimension_field %></td>
    <td><%= link_to 'Edit', edit_quality_dimension_field_path(quality_dimension_field) %></td>
    <td><%= link_to 'Destroy', quality_dimension_field, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

