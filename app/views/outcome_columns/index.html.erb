<h1>Listing outcome_columns</h1>

<table>
  <tr>
    <th>Outcome</th>
    <th>Name</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @outcome_columns.each do |outcome_column| %>
  <tr>
    <td><%= outcome_column.outcome_id %></td>
    <td><%= outcome_column.name %></td>
    <td><%= outcome_column.description %></td>
    <td><%= link_to 'Show', outcome_column %></td>
    <td><%= link_to 'Edit', edit_outcome_column_path(outcome_column) %></td>
    <td><%= link_to 'Destroy', outcome_column, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

